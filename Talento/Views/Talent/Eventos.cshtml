<div class="container mt-4" >
    <div class="row justify-content-center">
        <div>
            <iframe height="400px" width="800px" allowfullscreen="" frameborder="0" title="External YouTube" aria-label="External YouTube" src="//www.youtube.com/embed/i1G7ICAh8go?wmode=transparent&amp;autoplay=1&amp;theme=dark&amp;controls=1&amp;autohide=1&amp;loop=0&amp;showinfo=0&amp;rel=0&amp;playlist=false&amp;enablejsapi=0"></iframe>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6 offset-5 ">
            <input type="button" class="btn btn-primary btn-lg" name="btnYOUTUBE" value="REGISTRO DE EMPRESAS" />
        </div>
    </div>
    
</div>




    <script>
        // Load the IFrame Player API code asynchronously.
        var tag = document.createElement('script');
        tag.src = "https://www.youtube.com/watch?v=i1G7ICAh8go";
        var firstScriptTag = document.getElementsByTagName('script')[0];
        firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

        // Replace the 'ytplayer' element with an <iframe> and
        // YouTube player after the API code downloads.
        var player;
        function onYouTubePlayerAPIReady() {
            player = new YT.Player('ytplayer', {
                height: '480',
                width: '720',
                videoId: 'M7lc1UVf-VE'
            });
        }
    </script>
